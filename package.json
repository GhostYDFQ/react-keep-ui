{
  "name": "react-keep-ui",
  "version": "1.0.1",
  "private": false,
  "dependencies": {
    "@types/prismjs": "^1.16.1",
    "@types/react-dom": "^16.9.8",
    "axios": "^0.20.0",
    "coveralls": "^3.1.0",
    "polished": "^3.6.6",
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "styled-components": "^5.2.0",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "storybook": "start-storybook -p 6006 -s public",
    "build": "rm -rf ./dist && npm run build-all && npm run build-cjs",
    "build-cjs": "microbundle build --tsconfig tsconfig.build.json --jsx React.createElement --format cjs",
    "build-all": "microbundle build --tsconfig tsconfig.build.json --jsx React.createElement",
    "eject": "react-scripts eject",
    "build-storybook": "build-storybook --no-dll --quiet",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "coverall": "npm run coverage  && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "postinstall": "patch-package",
    "release": "npm run build && np"
  },
  "eslintConfig": {
    "extends": "react-app",
    "parser": "babel-eslint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-a11y": "^6.0.21",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addon-storysource": "^6.0.21",
    "@storybook/node-logger": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.21",
    "@testing-library/react": "^11.0.2",
    "@types/jest": "^26.0.13",
    "@types/styled-components": "^5.1.3",
    "babel-loader": "^8.1.0",
    "babel-register": "^6.26.0",
    "jest": "^26.4.2",
    "react-is": "^16.13.1",
    "styled-components": "^5.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/**/*.stories.tsx",
      "src/**/*.tsx",
      "!src/**/*.snap"
    ]
  },
  "files": [
    "dist"
  ],
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@GhostTDFQ"
  }
}
